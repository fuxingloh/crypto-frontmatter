{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build": {
      "inputs": ["tsconfig.json", "tsconfig.build.json", "./**/*.ts"],
      "outputs": ["./**/*.js", "./**/*.d.ts", "./**/*.d.ts.map"],
      "dependsOn": ["^build"]
    },
    "test": {
      "inputs": ["tsconfig.json", "./**/*.ts", "./**/*.unit.ts", "./tests-i9n/**", "./tests-e2e/**"],
      "dependsOn": ["^build"]
    }
  }
}
